{
  "name": "lacroix-web",
  "version": "0.0.0",
  "engines": {
    "node": "8.x.x"
  },
  "license": "MIT",
  "scripts": {
    "start": "node app",
    "server": "nodemon app",
    "server-mongo": "concurrently \"mongod --replSet rs0 --bind_ip localhost --port 27017\" \"nodemon app\"",
    "dev": "concurrently \"npm start --prefix client\" \"nodemon --inspect app\"",
    "local": "concurrently \"npm start --prefix client\" \"nodemon app\"",
    "local-serve": "concurrently \"npm run start:dev --prefix client\" \"nodemon app\"",
    "local-docker-windows": "concurrently \"docker exec -it atladb-dev bash -c 'mongo'\" \"npm start --prefix client\" \"nodemon app\"",
    "client-install": "npm install --prefix ./client",
    "client-build": "npm run build-prod --prefix ./client",
    "postinstall": "npm run client-install && npm run client-build"
  },
  "private": true,
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "@sendgrid/mail": "^6.4.0",
    "@swimlane/dragula": "^3.8.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "celebrate": "^10.0.1",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "express": "^4.16.2",
    "express-http-to-https": "^1.1.3",
    "firebase-admin": "^6.5.0",
    "firebase-functions": "^2.1.0",
    "font-awesome": "^4.7.0",
    "gridfs-stream": "^1.1.1",
    "handlebars": "^4.0.12",
    "html2pdf": "0.0.11",
    "html2pdf.js": "^0.9.0",
    "jsonwebtoken": "^8.1.1",
    "jwt-decode": "^2.2.0",
    "mailgun-js": "^0.22.0",
    "method-override": "^2.3.10",
    "mongoose": "^5.5.2",
    "mongoose-history": "^0.6.0",
    "mongoose-paginate-v2": "^1.3.0",
    "mongoose-version-history": "^0.3.0",
    "morgan": "^1.9.0",
    "multer": "^1.4.0",
    "multer-gridfs-storage": "^3.2.3",
    "npm": "^6.1.0",
    "pdfjs-dist": "^2.0.943",
    "print-js": "^1.0.52",
    "socket.io": "^2.2.0",
    "totalvoice-node": "^1.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jsdoc": "^3.5.5",
    "jsdoc-route-plugin": "^0.1.0",
    "nodemon": "^1.18.9",
    "prettier-eslint": "^9.0.0",
    "prettier-tslint": "^0.4.2"
  }
}
